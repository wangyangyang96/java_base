public class MyException extends Exception {
    private int value ;
//    public MyException(){}
    public MyException(int n){
        System.out.println("now you are in constuctor");
        value=n;
    }

    @Override
    public String toString() {
        System.out.println("now you are int toString");
        return "MyException{" +
                "value=" + value +
                '}';
    }
}
